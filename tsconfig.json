{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "/docs/",
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "./build_src",
    "pretty": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./*.js",
    "./*.ts",
    "./src/**/*.js",
    "./src/**/*.jsx",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}